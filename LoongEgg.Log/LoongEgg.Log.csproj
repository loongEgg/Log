  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net45|AnyCPU'">
    <DocumentationFile>E:\project\LoongEgg.Log\LoongEgg.Log\LoongEgg.Log.xml</DocumentationFile>
  </PropertyGroup>

